{
  "services": [
    {
      "name": "grafana",
      "tags": [
        "monitoring"
      ],
      "port": 3000,
      "check": {
        "name": "Health check for Grafana on port 3000",
        "http": "http://localhost:3000",
        "interval": "10s"
      }
    },
    {
      "name": "prometheus",
      "tags": [
        "monitoring"
      ],
      "port": 9090,
      "check": {
        "name": "Health check for Prometheus on port 9090",
        "http": "http://localhost:9090",
        "interval": "10s"
      }
    }
  ]
}